<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.xforceplus.wapp.modules.job.dao.TAcOrgDao" >

    <select id="getTaxnoByType"  resultType="com.xforceplus.wapp.modules.job.entity.TAcOrg">
        select taxno , link_name  linkName from t_ac_org WITH(NOLOCK) where orgtype = ${type} and taxno != '0'
    </select>
</mapper>