<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.xforceplus.wapp.modules.redTicket.dao.RedTicketMatchMiddleDao">

    <resultMap id="redTicketMatchMiddleMap" type="com.xforceplus.wapp.modules.redTicket.entity.RedTicketMatchMiddle">
        <result column="invoice_code" property="invoiceCode"/>
        <result column="invoice_no" property="invoiceNo"/>
        <result column="red_rush_amount" property="redRushAmount"/>
        <result column="tax_rate" property="taxRate"/>
        <result column="tax_amount" property="taxAmount"/>
        <result column="invoice_date" property="invoiceDate"/>
        <result column="gf_tax_no" property="gfTaxNo"/>
        <result column="total_amount" property="totalAmount"/>
        <result column="red_ticket_matching_association" property="redTicketMatchingAssociation"/>
    </resultMap>


    <insert id="insertRedTicketMatchMiddle" parameterType="com.xforceplus.wapp.modules.redTicket.entity.RedTicketMatchMiddle" useGeneratedKeys="true" >
    insert into t_dx_red_ticket_matching_middle
    (
    invoice_code,
    invoice_no,
    invoice_type,
    invoice_amount,
    red_rush_amount,
    tax_rate,
    tax_amount,
    invoice_date,
    gf_tax_no,
    total_amount,
    red_ticket_matching_association
    ) values(
    #{map.invoiceCode},
    #{map.invoiceNo},
    #{map.invoiceType},
    #{map.invoiceAmount},
    #{redRushAmount},
    #{map.taxRate},
    #{map.taxAmount},
    #{map.invoiceDate},
    #{map.gfTaxNo},
    #{map.totalAmount},
    #{redTicketMatchingId}
    )
    </insert>


</mapper>