<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.xforceplus.wapp.modules.redTicket.dao.ReturnAgreementMiddleDao">

    <resultMap id="returnAgreementMiddleMap" type="com.xforceplus.wapp.modules.redTicket.entity.ReturnAgreementMiddle">
        <result column="red_ticket_matching_association" property="redTicketMatchingAssociation"/>
        <result column="return_agreement_association" property="returnAgreementAssociation"/>
    </resultMap>


    <insert id="insertReturnAgreementMiddle" parameterType="com.xforceplus.wapp.modules.redTicket.entity.RedTicketMatchMiddle" useGeneratedKeys="true" keyProperty="map.id">
    insert into t_dx_return_agreement_middle
    (
    red_ticket_matching_association,
    return_agreement_association
    ) values(
    #{map.redTicketMatchingAssociation},
    #{map.returnAgreementAssociation}
    )
    </insert>


</mapper>