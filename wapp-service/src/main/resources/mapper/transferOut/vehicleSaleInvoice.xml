<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xforceplus.wapp.modules.transferOut.dao.DetailDao">
    <resultMap id="orgResultMap" type="com.xforceplus.wapp.modules.transferOut.entity.DetailVehicleEntity">
        <id column="id" property="id"/>
        <result column="uuid" property="uuid"/>
        <result column="buyer_id_num" property="buyerIdNum"/>
        <result column="vehicle_type" property="vehicleType"/>
        <result column="factory_model" property="factoryModel"/>
        <result column="product_place" property="productPlace"/>
        <result column="certificate" property="certificate"/>
        <result column="certificate_import" property="certificateImport"/>
        <result column="inspection_num" property="inspectionNum"/>
        <result column="engine_no" property="engineNo"/>
        <result column="vehicle_no" property="vehicleNo"/>
        <result column="phone" property="phone"/>
        <result column="buyer_bank" property="buyerBank"/>
        <result column="tax_rate" property="taxRate"/>
        <result column="tax_bureau_name" property="taxBureauName"/>
        <result column="tax_bureau_code" property="taxBureauBode"/>
        <result column="tax_records" property="taxRecords"/>
        <result column="limit_people" property="limitPeople"/>
        <result column="check_status" property="checkStatus"/>
        <result column="tonnage" property="tonnage"/>
        <result column="create_date" property="createDate"/>
    </resultMap>

<select id="getVehicleDetail" resultType="com.xforceplus.wapp.modules.transferOut.entity.DetailVehicleEntity">
/**mycat:schema=${schemaLabel}*/
    SELECT
    *
    FROM t_dx_vehicle_sale_invoice
    WHERE uuid IN (SELECT uuid FROM t_dx_record_invoice WITH(NOLOCK) WHERE id=#{id})
</select>

<select id="getCheckVehicleDetail" resultType="com.xforceplus.wapp.modules.transferOut.entity.DetailVehicleEntity">
    /**mycat:schema=${schemaLabel}*/
    SELECT
    *
    FROM t_dx_check_vehicle_sale_invoice
    WHERE uuid IN (SELECT uuid FROM t_dx_check_record_invoice WITH(NOLOCK) WHERE id=#{id})
</select>


</mapper>